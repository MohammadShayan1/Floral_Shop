
@{
    ViewData["Title"] = "Create Products";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model Category




<form method="Post">

    <input type="hidden" asp-for="C_Id" />

    <label asp-for="C_Name">Category Name</label>
    <input disabled asp-for="C_Name" type="text" class="form-control" />

    <label asp-for="Display_Order">Display Order</label>
    <input disabled asp-for="Display_Order" type="number" class="form-control" />
    <br>
    <button class="btn btn-danger" type="submit">Delete Category</button>

</form>
